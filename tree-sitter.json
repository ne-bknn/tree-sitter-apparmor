{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "apparmor",
      "camelcase": "AppArmor",
      "title": "AppArmor",
      "scope": "source.apparmor",
      "file-types": [
        "apparmor",
        "aa"
      ],
      "injection-regex": "^apparmor$",
      "class-name": "TreeSitterApparmor"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Apparmor grammar for tree-sitter",
    "authors": [
      {
        "name": "Timofei Mishchenko",
        "email": "me@ne-bknn.com"
      }
    ],
    "links": {
      "repository": "https://github.com/ne-bknn/tree-sitter-apparmor"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}